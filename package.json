{
  "name": "botai",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "tsx ./server.ts | pino-pretty",
    "start": "cross-env NODE_ENV=production tsx ./server.ts",
    "docker": "npm run db-push && cross-env NODE_ENV=production tsx ./server.ts",
    "docs": "typedoc --options typedoc.json",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "db-studio": "prisma studio",
    "db-generate": "zenstack generate",
    "db-push": "prisma db push",
    "db-migrate-dev": "prisma migrate dev",
    "db-migrate-deploy": "prisma migrate deploy",
    "db-migrate-status": "prisma migrate status",
    "proxy-redis": "fly redis proxy",
    "proxy-db": "fly proxy 5432:5432 --app botai-db"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hcaptcha/react-hcaptcha": "^1.11.0",
    "@headlessui/react": "^2.1.3",
    "@prisma/client": "^5.18.0",
    "@remix-run/express": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@sentry/remix": "^8.27.0",
    "@sentry/vite-plugin": "^2.22.3",
    "@types/express-session": "^1.18.0",
    "@types/react-tag-input": "^6.6.6",
    "@zenstackhq/runtime": "2.4.1",
    "argon2": "^0.40.3",
    "avvvatars-react": "^0.4.2",
    "baileys": "github:WhiskeySockets/Baileys",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-slow-down": "^2.0.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "is-ip": "^5.0.1",
    "isbot": "^4.1.0",
    "mongodb": "^6.8.1",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.14",
    "openai": "^4.57.0",
    "pino": "^9.4.0",
    "prisma": "^5.18.0",
    "qrcode.react": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-tag-input": "^6.10.3",
    "remix-auth": "^3.7.0",
