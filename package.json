    "socket.io-client": "^4.7.5",
    "string-similarity": "^4.0.4",
    "tsx": "^4.19.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.11.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/nodemailer": "^6.4.15",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
